includes:
    - ./vendor/larastan/larastan/extension.neon

parameters:

    paths:
        - src

    # The level 9 is the highest level
    level: 9

    ignoreErrors:
        - '#Call to an undefined method property object::\$headers#'
        - '#Call to an undefined method property object::\$status#'
    excludePaths:
        - ./*/*/FileToBeExcluded.php

    checkMissingIterableValueType: false
